{
  "name": "hunt2025",
  "private": true,
  "version": "0.0.1",
  "description": "HTTP frontend for the 2025 MIT Mystery Hunt website",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "rspack --mode=production --config ./rspack.config.mjs",
    "build-dev": "rspack --mode=development --config ./rspack.config.mjs",
    "start": "npm run build && node ./dist/server-bundle.js",
    "start-watch": "npm run build-dev && concurrently --kill-others \"node --watch --enable-source-maps ./dist/server-bundle.js\" \"rspack --mode=development --config ./rspack.config.mjs --watch\" \"livereload ./dist -w 3000\"",
    "check": "tsc && tsc -p worker/tsconfig.json && tsc -p ops/tsconfig.json && tsc -p ops/server/tsconfig.json && prettier . --check && eslint .",
    "format": "prettier . --write && eslint . --fix",
    "knex": "node --import tsx/esm ./node_modules/.bin/knex",
    "test": "jest",
    "build-all": "npm run build && npm run ops:vite-build",
    "ops:build": "cd ops && rm -rf ../dist-ops && tsc -p ./server/tsconfig.json && vite build",
    "ops:vite-build": "cd ops && vite build",
    "ops:dev-server": "cd ops && NODE_ENV=development node --watch --import tsx/esm ./server/main.ts",
    "ops:start": "cd ops && concurrently --kill-others vite \"npm run ops:dev-server\"",
    "install-hooks": "cd .. && husky site/.husky"
  },
  "lint-staged": {
    "*": [
      "prettier --check",
      "eslint --no-warn-ignored"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.699.0",
